 103:controller/utils/comms.c **** 
 104:controller/utils/comms.c **** 	/* Form the packet in the transmit buffer */
 105:controller/utils/comms.c **** 	size_t tx_buffer_index = 0;
 106:controller/utils/comms.c **** 	memcpy(tx_buffer, &prefix, sizeof(prefix)); // prefix to buffer
 107:controller/utils/comms.c **** 	tx_buffer_index += sizeof(prefix);
 108:controller/utils/comms.c **** 	memcpy(tx_buffer + tx_buffer_index, &flag, sizeof(flag)); // flag to buffer 
 109:controller/utils/comms.c **** 	tx_buffer_index += sizeof(flag);
 110:controller/utils/comms.c **** 	memcpy(tx_buffer + tx_buffer_index, buf, size); // payload to buffer
 111:controller/utils/comms.c **** 	tx_buffer_index += size;
 112:controller/utils/comms.c **** 	memcpy(tx_buffer + tx_buffer_index, &suffix, sizeof(suffix)); // suffix to buffer
 113:controller/utils/comms.c **** 	tx_buffer_index += sizeof(suffix);
 114:controller/utils/comms.c **** 	
 115:controller/utils/comms.c **** 	/* Finally transmit over USB */
 116:controller/utils/comms.c **** 
 117:controller/utils/comms.c **** 	CDC_Transmit_FS(tx_buffer, packet_size);
